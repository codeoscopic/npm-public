{
  "name": "@codeoscopic/vite-config",
  "version": "1.0.0",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/codeoscopic/npm-public.git"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
