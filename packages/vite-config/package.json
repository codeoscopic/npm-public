    "@vitejs/plugin-react": "^1.3.0",
    "vite": "^2.6.14",
    "less": "^4.1.2",
    "less-loader": "^10.2.0"
  },
  "optionalDependencies": {
    "@vitejs/plugin-react": "^1.3.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0"
  }
